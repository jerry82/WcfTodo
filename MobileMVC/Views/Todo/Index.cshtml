@model IEnumerable<MobileMVC.wcfService.Category>

@{
    ViewBag.Title = "Category";
}

<ul data-role="listview">

</ul>

<ul data-role="listview" data-divider-theme="b" data-inset="false">
    <li data-role="list-divider" role="heading">Lists</li>
    @foreach (var cat in Model)
    {
        <li data-theme="c">@Html.ActionLink(cat.Title, "Items", new { cat.Id })</li>
    }
</ul>

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}